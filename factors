#!/usr/bin/python3
from sys import argv
from math import sqrt


def factor():
    with open(argv[1]) as f:
        for line in f:
            num = int(line)
            print("{:d}=".format(num), end="")
            if num % 2 == 0:
                print("{}*2".format(num//2))
                continue
            sqrt_n = int(sqrt(num))
            if sqrt_n % 2 == 0:
                sqrt_n += 1
            for i in range(3, sqrt_n + 1, 2):
                if num % i == 0:
                    print("{}*{}".format(i, num//i))
                    break
            if num % i != 0:
                print("{}={}*1".format(num, num))


factor()
